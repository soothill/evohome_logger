# Copy this file to config.env and fill in your credentials before running.
EVOHOME_USERNAME=you@example.com
EVOHOME_PASSWORD=your-password

# InfluxDB connection details
INFLUX_URL=http://influxdb:8086
INFLUX_BUCKET=evohome
INFLUX_ORG=your-org
INFLUX_TOKEN=your-token

# Optional overrides
#EVOHOME_LOCATION_INDEX=0
#INFLUX_VERIFY_TLS=true
#DATA_DIR=/data
#HTTP_TIMEOUT_MS=10000
